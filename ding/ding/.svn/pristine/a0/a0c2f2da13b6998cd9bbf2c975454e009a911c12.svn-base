      #{statusRef,jdbcType=VARCHAR}, #{dueDate,jdbcType=VARCHAR}, #{tranDate,jdbcType=VARCHAR}
      )
      on duplicate key update cust_body_approver =#{custBodyApprover}, quantity = #{quantity}, rate = #{rate}
  </insert>
  
  <!-- 根据订单id查询审批id -->
